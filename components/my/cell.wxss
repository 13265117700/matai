
.cell-item{
    display: flex;
    align-items: center;
    height: 60px;
    line-height: 60px;
    position: relative;
    padding-left: 15px;
}
.cell-text{
    flex: 1;
    margin-left: 23px;
    font-size: 18px;
    font-family: PingFang-SC-Medium;
    font-weight: medium;
    color: #191919FF;
    border-bottom: 1px solid #E5E5E5FF;
}
/* .cell-icon{
    width: 20px;
    height: 16.5px;
} */
.cell-icon image{
    width: 20px;
    height: 16.5px;
}
.cell-arrow{
    position: absolute;
    top: 50%;
    right: 15.5px;
    transform: translateY(-50%);
}
.cell-arrow image{
    width: 6px;
    height: 10px;
}