page {
  padding-bottom: 60px;
}
.title {
  color: #000000;
  font-size: 44rpx;
  padding: 20rpx;
}

.fiedx-bottom {
  position: fixed;
  width: 100%;
  height: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  left: 0;
  bottom: 0;
  padding: 20rpx 30rpx;
}

.fiedx-bottom .btn {
  color: #fff;
  background-color: #25C3CF;
  font-size: 32rpx;
  line-height: 1;
  flex: 1;
  padding: 24rpx 0;
  text-align: center;
  border-radius: 40rpx;
}
