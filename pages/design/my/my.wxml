<view hidden="{{ !showGray }}" catchtouchmove="touchmove" class="kf">
	<view hidden="{{ !showCover }}" catchtap="cancelBtn" class="cover-bg"></view>
	<view class="kf-container {{ showCover ? 'show' : '' }}">
		<view class="kf-list">
			<view class="kf-item" catchtap="doCall">
				<view>400-688-4898</view>
				<view>周一至周五 9:30 - 20:30</view>
			</view>
			<view class="kf-item online">
				<view>在线客服</view>
				<view>周一至周五 9:30 - 20:30</view>
				<button class="contact-btn" open-type="contact"></button>
			</view>
		</view>
		<view class="cancel-btn" catchtap="cancelBtn">取消</view>
	</view>
</view>