page {
  background-color: #fff;
}


.top {
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
  background-color: white;
  background-size: 100% auto;
  background-repeat: no-repeat;
  /* 从这里往上为设置自定义导航栏 */
  height: 330rpx;
}

.top .info {
  position: relative;
  top: 160rpx;
  height: 200rpx;
  background-color: #fff;
  background-size: cover;
  display: flex;
  align-items: center;
  border-radius: 20rpx;
  margin: 30rpx;
  padding-left: 40rpx;
  font-size: 28rpx;
  color: #4B2E0B;
}

.top .info .nick-name {
  display: flex;
  align-items: center;
}

.top .info .nick-name .vip-type {
  display: flex;
  align-items: center;
  color: #FFE8C3;
  font-size: 18rpx;
  position: relative;
  margin-left: 20rpx;
}

.top .info .nick-name .f {
  height: 50rpx;
  max-width: 300rpx;
  display: block;
}

.top .info .nick-name .abs {
  position: absolute;
  left: 42rpx;
  line-height: 1;
  margin-top: 4rpx;
}

